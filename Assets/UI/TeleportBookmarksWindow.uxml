                <ui:TextField picking-mode="Ignore" label="New Bookmarks List" value="..." name="bookmarks-list-name" style="width: 75%; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;" />
                <ui:Button text="Create" display-tooltip-when-elided="true" name="create-bookmarks-list" style="height: 35px; width: 20%;" />
